<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>com.agent-bridge.server</string>

    <key>ProgramArguments</key>
    <array>
      <string>/Users/MN/GITHUB/.agent-bridge/venv/bin/python</string>
      <string>/Users/MN/GITHUB/.agent-bridge/server.py</string>
      <string>--port</string>
      <string>5111</string>
      <string>--no-debug</string>
    </array>

    <key>WorkingDirectory</key>
    <string>/Users/MN/GITHUB/.agent-bridge</string>

    <key>EnvironmentVariables</key>
    <dict>
      <key>PYTHONUNBUFFERED</key>
      <string>1</string>
    </dict>

    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>

    <key>StandardOutPath</key>
    <string>/tmp/agent-bridge-server.log</string>
    <key>StandardErrorPath</key>
    <string>/tmp/agent-bridge-server.err.log</string>
  </dict>
</plist>
